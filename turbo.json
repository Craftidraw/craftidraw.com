{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "!.next/cache/**", "apps/docs/build/**"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}
